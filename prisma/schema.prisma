generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Project {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  title       String
  description String
  image       String
  alt         String
  githubLink  String
  deployLink  String
  projectDate String
}
